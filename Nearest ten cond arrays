import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       int n; 
        Scanner s=new Scanner (System.in);
        n=s.nextInt();
       int remainder = n % 10;
      if (remainder >= 5) {
      System.out.println(+( n + (10 - remainder)));
}
        else
        {
  System.out.println( +( n - remainder));
}
     
    }
}
