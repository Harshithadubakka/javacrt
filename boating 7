import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int m, a, c;
      Scanner s =new Scanner(System.in);
        m=s.nextInt();
        a=s.nextInt();
        c=s.nextInt();
        int w = a * 75 + c * 30;
        if (w <= m)
        { 
            System.out.println("Boat is stable");
        }
        else
        {
           System.out.println  ("Boat will drown");
        }
       
    }
}
