import java.util.*;
class test

{
    public static void main( String args[])

    {
        Scanner s=new Scanner(System.in);
        String s1;
        s1=s.nextLine();
        char ch[]=s1.toCharArray();
        for(int i=0;i<ch.length;i++)
        {
          System.out.println(ch[i]); 
        }
       
       
    }
}
